<script>

$(document).ready(function() {
    
    $('.selectable').click(function() {
        
        var step_number = +$(this).first().parent().parent().attr('data-step');
        $('.step[data-step = ' + step_number + ']').children().children().removeClass('selected');
        $(this).addClass('selected');
        
        calculate_price();
    });

    $('.price_collumn').pin({minWidth: 940});
    
    $('.rest').click(function(){
        $('html, body').animate({scrollTop: $('#start').offset().top}, 300);
    })
    
    $('.chassis_container').children().click(function() {
        $('.chassis_container').children().removeClass('selected');
        $(this).addClass('selected');
    });

    $('.tires_container').children().click(function() {
        $('.tires_container').children().removeClass('selected');
        $(this).addClass('selected');
        calculate_price();
    });

    $('.single_selectable').click(function() {
        $(this).toggleClass('selected');
        calculate_price();
    });

    $('.chassis_container').children().click(function() {
        var color = $(this).attr('data-color');
        var tire = $('.tires_container').children().filter('.selected').attr('data-tire');
        if (tire === undefined) {
            tire = "1";
        } else {
            tire = $('.tires_container').children().filter('.selected').attr('data-tire');
        }
        $("#image").reel("images", "/assets/Animated_all/" + color + "/" + color + "_" + tire + "_tire/" + "frame_##.jpg");
        calculate_price();
    });

    $('.tires_container').children().click(function() {
        var tire = $(this).attr('data-tire');
        var color = $('.chassis_container').children().filter('.selected').attr('data-color');
        if (color === undefined) {
            color = "pink";
        } else {
            color = $('.chassis_container').children().filter('.selected').attr('data-color');
        }
        $("#image").reel("images", "/assets/Animated_all/" + color + "/" + color + "_" + tire + "_tire/" + "frame_##.jpg");
        calculate_price();
    });

    function calculate_price() {
        var price = 0;
        for (var i = 0; i < $('.selected').length; i++) {
            price += parseInt($('.selected')[i].getAttribute('data-price'), 10);
        }

        $('.price_result').html('$' + price);
        
        var after = price - 21500
        $(".price_after_savings").html("$"+ after);

    }
    
});
</script>